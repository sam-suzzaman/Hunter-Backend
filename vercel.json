{
    "version": 2,
    "builds": [
        {
            "src": "Server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/Server.js",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/Server.js",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
